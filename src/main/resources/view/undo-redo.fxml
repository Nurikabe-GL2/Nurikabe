<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="500.0" prefWidth="800.0" type="VBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox id="contentBox" alignment="CENTER" VBox.vgrow="ALWAYS">
         <children>
            <Pane VBox.vgrow="ALWAYS" />
            <Pane VBox.vgrow="ALWAYS" />
            <VBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#displayAction" prefHeight="40.0" prefWidth="250.0" text="Afficher" />
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#undoAction" prefHeight="40.0" prefWidth="120.0" text="Undo" />
                        <Button mnemonicParsing="false" onAction="#redoAction" prefHeight="40.0" prefWidth="120.0" text="Redo" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Pane VBox.vgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../css/style.css" />
      <URL value="@../css/ScrollPane.css" />
      <URL value="@../css/MenuPrincipal.css" />
   </stylesheets>
</fx:root>
